      "source": [
        "def calc_error(y, y_pred):\n",
        "    total = 0\n",
        "    for a, b in zip(y, y_pred):\n",
        "        total += (a - b) ** 2\n",
        "    return total\n",
        "\n",
        "y = [1, 2, 3]\n",
        "y_pred = [2, 2, 4]\n",
        "print(\"Error value:\", calc_error(y, y_pred))"
      ]
    }
